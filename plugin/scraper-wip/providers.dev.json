      "route": "/github/stars",
      "debug": false,
      "disabled": false,
      "cache": true,
      "extract_paths": true,
      "name": "github-stars",
      "provider": "github.com",
      "base_url": "https://api.github.com",
      "url": "/users/{{username}}/starred?per_page={{per_page}}&page={{page}}&order=desc&direction=desc",
      "examples": {
        "docs": {
          "desc": "ListStarred lists all the repos starred by a user.",
          "link": "https://developer.github.com/v3/activity/starring/#list-repositories-being-starred"
        },
        "remote": {
          "desc": "",
          "link": "https://api.github.com/users/{{GITHUB_ACCOUNT}}/starred?per_page=5&page=1&order=desc&direction=desc"
        },
        "local": {
          "desc": "",
          "link": "http://localhost:3000/github/search?query=dlib"
        }
      },
      "timeout": 3,
      "concurrency": 20,
      "collection": true,
      "pager": {
        "disabled": "false",
        "max": "500",
        "offset": "0",
        "limit": "100",
        "offset_var": "page",
        "limit_var": "per_page",
        "limit_checker": "per_page"
      },
      "selector": "json",
      "method": "GET",
      "headers": {
        "Authorization": " token {{GITHUB_TOKEN}}",
        "Accept": "application/vnd.github.mercy-preview+json"
      },
      "resp_headers_intercept": [
        "Link",
        "X-Github-Media-Type",
        "X-Ratelimit-Limit",
        "X-Ratelimit-Remaining",
        "X-Ratelimit-Reset",
        "Content-Length"
      ],
      "blocks": {
        "result": {
          "items": "items",
          "cache": true,
          "details": {
            "id": "id",
            "name": "name",
            "desc": "description",
            "owner": [
              "owner.avatar_url",
              "owner.id",
              "owner.login"
            ],
            "full_name": "full_name",
            "topics": "topics"
          }
        }
      }
    },
    {
      "route": "/github/search",
      "debug": false,
      "disabled": false,
      "extract_paths": true,
      "cache": true,
      "name": "github-search",
      "provider": "github.com",
      "base_url": "https://api.github.com",
      "url": "/search/repositories?q={{query}}&per_page={{per_page}}&page={{page}}&order=desc&sort=stars",
      "examples": {
        "remote": {
          "desc": "",
          "link": "https://api.github.com/search/repositories?q=dlib&per_page=1&page=1&order=desc&sort=stars"
        },
        "local": {
          "desc": "",
          "link": "http://localhost:3000/github/search?query=dlib"
        }
      },
      "concurrency": 20,
      "pager": {
        "disabled": "false",
        "max": "20",
        "offset": "1",
        "limit": "100",
        "offset_var": "page",
        "limit_var": "per_page"
      },
      "timeout": 3,
      "selector": "json",
      "method": "GET",
      "headers": {
        "Authorization": " token {{GITHUB_TOKEN}}",
        "Accept": "application/vnd.github.mercy-preview+json"
      },
      "parameters": {
        "per_page": {
          "default": "100",
          "max": "100",
          "type": "query_string"
        },
        "page": {
